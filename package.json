  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon ./src/main.ts",
    "start": "node ./dist/main.js",
    "test:watch": "jest --coverage --verbose --watchAll",
    "test": "jest --verbose --passWithNoTests",
