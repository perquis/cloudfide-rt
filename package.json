    "mongoose": "^8.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/whatwg-url": "^11.0.5",
    "commitizen-emoji": "^1.0.5",
